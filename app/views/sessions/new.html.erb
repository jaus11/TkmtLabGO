<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :name_id %>
      <%= f.text_field :name_id%><br>

      <%= f.label :password %>
      <%= f.password_field :password%><br>

      <%= f.submit "Log in"%>
    <% end %>

  </div>
</div>
